####### Install

install_target: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/opt/BuksanSpy/bin/ || $(MKDIR) $(INSTALL_ROOT)/opt/BuksanSpy/bin/ 
	-$(INSTALL_PROGRAM) "$(QMAKE_TARGET)" "$(INSTALL_ROOT)/opt/BuksanSpy/bin/$(QMAKE_TARGET)"
	-$(STRIP) "$(INSTALL_ROOT)/opt/BuksanSpy/bin/$(QMAKE_TARGET)"

uninstall_target:  FORCE
	-$(DEL_FILE) "$(INSTALL_ROOT)/opt/BuksanSpy/bin/$(QMAKE_TARGET)"
	-$(DEL_DIR) $(INSTALL_ROOT)/opt/BuksanSpy/bin/ 


install:  install_target  FORCE

uninstall: uninstall_target   FORCE

FORCE:

